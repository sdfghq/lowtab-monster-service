{
  "runtime": "$(Runtime)",
  "documentGenerator": {
    "fromDocument": {
      "url": "$(SpecUrl)"
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "generateClientClasses": true,
      "generateClientInterfaces": true,
      "injectHttpClient": true,
      "disposeHttpClient": false,
      "generateDtoTypes": false,
      "useHttpClientCreationMethod": false,
      "useBaseUrl": false,
      "generateBaseUrlProperty": true,
      "operationGenerationMode": "SingleClientFromOperationId",
      "output": "Generated/LowtabMonsterServiceClient.cs",
      "namespace": "Lowtab.Monster.Service.Api.Client",
      "className": "LowtabMonsterServiceClient",
      "jsonLibrary": "SystemTextJson",
      "useNullableReferenceTypes": true,
      "generateOptionalPropertiesAsNullable": true,
      "exceptionClass": "LowtabMonsterServiceClientException",
      "generateDefaultValues": false,
      "additionalNamespaceUsages": [
        "Lowtab.Monster.Service.Contracts.Articles.AddTagToArticle",
        "Lowtab.Monster.Service.Contracts.Articles.CreateArticle",
        "Lowtab.Monster.Service.Contracts.Articles.DeleteArticle",
        "Lowtab.Monster.Service.Contracts.Articles.DeleteTagFromArticle",
        "Lowtab.Monster.Service.Contracts.Articles.GetArticle",
        "Lowtab.Monster.Service.Contracts.Articles.GetArticles",
        "Lowtab.Monster.Service.Contracts.Articles.UpdateArticle",
        "Lowtab.Monster.Service.Contracts.GroupTags",
        "Lowtab.Monster.Service.Contracts.Tags.CreateTag",
        "Lowtab.Monster.Service.Contracts.Tags.DeleteTag",
        "Lowtab.Monster.Service.Contracts.Tags.GetTag",
        "Lowtab.Monster.Service.Contracts.Tags.GetTags",
        "Lowtab.Monster.Service.Contracts.Tags.UpdateTag"
      ]
    }
  }
}
